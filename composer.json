{
    "name": "blackcms/assets",
    "description": "Assets Management Composer Package",
    "keywords": [
        "blackcms",
        "assets"
    ],
    "homepage": "https://github.com/pyorteam/blackcms-assets",
    "support": {
        "issues": "https://github.com/pyorteam/blackcms-assets/issues"
    },
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8|^9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "BlackCMS\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlackCMS\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlackCMS\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "BlackCMS\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
